{
    "version": 2,
    "builds": [
      {
        "src": "server/client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "server/client/build"
        }
      },
      {
        "src": "server/App.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/App.js"
      },
      {
        "src": "/(.*)",
        "dest": "/server/client/$1"
      }
    ]
  }
  